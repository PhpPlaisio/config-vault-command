{
  "name": "plaisio/config-vault-command",
  "description": "PhpPlaisio: Config Vault Commands",
  "keywords": [
    "PhpPlaisio",
    "Config Vault"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "plaisio/config-vault": "^1.3.0",
    "plaisio/console": "3.0.0",
    "plaisio/kernel": "^3.2.0",
    "setbased/exception": "^2.3.0",
    "setbased/helper-cast": "^3.0.0",
    "symfony/console": "^6.2.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/console-kernel": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ConfigVault\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\ConfigVault\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
